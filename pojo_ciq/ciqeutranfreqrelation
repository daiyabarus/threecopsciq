        scr += f"threshLow_3: {self.threshLow_3}" + "\n"
        scr += f"redirectionOrder_3: {self.redirectionOrder_3}" + "\n"
        scr += f"RelationId_4: {self.RelationId_4}" + "\n"
        scr += f"earfcnDL_4: {self.earfcnDL_4}" + "\n"
