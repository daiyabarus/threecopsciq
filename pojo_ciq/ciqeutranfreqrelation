        self.RNC = RNC
        self.Utrancell = Utrancell
        self.eutraDetection = eutraDetection
        self.RelationId_1 = RelationId_1
        self.earfcnDL_1 = earfcnDL_1
        self.cellReselectionPriority_1 = cellReselectionPriority_1
        self.qRxLevMin_1 = qRxLevMin_1
        self.threshHigh_1 = threshHigh_1
        self.threshLow_1 = threshLow_1
        self.redirectionOrder_1 = redirectionOrder_1
        self.RelationId_2 = RelationId_2
        self.earfcnDL_2 = earfcnDL_2
        self.cellReselectionPriority_2 = cellReselectionPriority_2
        self.qRxLevMin_2 = qRxLevMin_2
        self.threshHigh_2 = threshHigh_2
        self.threshLow_2 = threshLow_2
        self.redirectionOrder_2 = redirectionOrder_2
        self.RelationId_3 = RelationId_3
        self.earfcnDL_3 = earfcnDL_3
        self.cellReselectionPriority_3 = cellReselectionPriority_3
        self.qRxLevMin_3 = qRxLevMin_3
        self.threshHigh_3 = threshHigh_3
        self.threshLow_3 = threshLow_3
        self.redirectionOrder_3 = redirectionOrder_3
        self.RelationId_4 = RelationId_4
        self.earfcnDL_4 = earfcnDL_4
        self.cellReselectionPriority_4 = cellReselectionPriority_4
        self.qRxLevMin_4 = qRxLevMin_4
        self.threshHigh_4 = threshHigh_4
        self.threshLow_4 = threshLow_4
