        scr += f"earfcnDL_1: {self.earfcnDL_1}" + "\n"
        scr += f"cellReselectionPriority_1: {self.cellReselectionPriority_1}" + "\n"
        scr += f"qRxLevMin_1: {self.qRxLevMin_1}" + "\n"
        scr += f"threshHigh_1: {self.threshHigh_1}" + "\n"
        scr += f"threshLow_1: {self.threshLow_1}" + "\n"
        scr += f"redirectionOrder_1: {self.redirectionOrder_1}" + "\n"
        scr += f"RelationId_2: {self.RelationId_2}" + "\n"
        scr += f"earfcnDL_2: {self.earfcnDL_2}" + "\n"
        scr += f"cellReselectionPriority_2: {self.cellReselectionPriority_2}" + "\n"
        scr += f"qRxLevMin_2: {self.qRxLevMin_2}" + "\n"
        scr += f"threshHigh_2: {self.threshHigh_2}" + "\n"
