class EutranFreqRelationIndex:
    def __init__(self) -> None:
        self.RNC = 0
        self.Utrancell = 1
        self.eutraDetection = 2
        self.RelationId_1 = 3
        self.earfcnDL_1 = 4
        self.cellReselectionPriority_1 = 5
        self.qRxLevMin_1 = 6
        self.threshHigh_1 = 7
        self.threshLow_1 = 8
        self.redirectionOrder_1 = 9
        self.RelationId_2 = 10
        self.earfcnDL_2 = 11
        self.cellReselectionPriority_2 = 12
        self.qRxLevMin_2 = 13
        self.threshHigh_2 = 14
        self.threshLow_2 = 15
        self.redirectionOrder_2 = 16
        self.RelationId_3 = 17
        self.earfcnDL_3 = 18
