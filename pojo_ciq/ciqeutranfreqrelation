    @classmethod
    def from_row(cls, row: any, idx: EutranFreqRelationIndex):
        return cls(
            RNC=gval(row[idx.RNC]),
            Utrancell=gval(row[idx.Utrancell]),
            eutraDetection=gval(row[idx.eutraDetection]),
            RelationId_1=gval(row[idx.RelationId_1]),
