            qRxLevMin_3=gval(row[idx.qRxLevMin_3]),
            threshHigh_3=gval(row[idx.threshHigh_3]),
            threshLow_3=gval(row[idx.threshLow_3]),
            redirectionOrder_3=gval(row[idx.redirectionOrder_3]),
            RelationId_4=gval(row[idx.RelationId_4]),
