            redirectionOrder_1=gval(row[idx.redirectionOrder_1]),
            RelationId_2=gval(row[idx.RelationId_2]),
            earfcnDL_2=gval(row[idx.earfcnDL_2]),
            cellReselectionPriority_2=gval(row[idx.cellReselectionPriority_2]),
            qRxLevMin_2=gval(row[idx.qRxLevMin_2]),
            threshHigh_2=gval(row[idx.threshHigh_2]),
            threshLow_2=gval(row[idx.threshLow_2]),
